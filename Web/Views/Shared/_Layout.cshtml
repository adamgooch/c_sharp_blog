<!DOCTYPE html>
  <!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
  <!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
  <!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
  <!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/assets/styles/bootstrap.min.css")
    @Styles.Render("~/assets/styles/bootstrap-responsive.min.css")
    @Styles.Render("~/assets/styles/main.min.css")
    @Scripts.Render("~/assets/scripts/modernizr.custom.min.js")
  </head>
  <body>
    <div id="wrap">
      <div class="container-fluid">
        <header class="container-fluid">
          <div class="navbar">
            <div class="navbar-inner">
              <div class="row-fluid">
                <div class="span10">
                  <h1>Adam Gooch</h1>
                </div>
                <div class="pull-right login">
                  <!-- @Html.Partial("_LoginPartial") -->
                </div>
                <div class="row-fluid">
                  <div class="span12">
                    <ul class="nav">
                      
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </header>

        <div id="content" class="container-fluid">
          @RenderSection("featured", required: false)
          <section class="main-content">
            @RenderBody()
          </section>
        </div>
      </div>
      <div id="push"></div>
    </div>
    <div id="footer">
      <p class="muted footer">&copy; @DateTime.Now.Year - All Rights Reserved</p>
    </div>
    
    @Scripts.Render("~/assets/scripts/jquery-1.9.1.min.js")
    @Scripts.Render("~/assets/scripts/bootstrap.min.js")
    @RenderSection("PageLevelJavascript", required: false)
  </body>
</html>
